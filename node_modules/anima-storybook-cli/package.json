{
  "name": "anima-storybook-cli",
  "version": "0.13.1",
  "description": "Anima CLI for storybook",
  "main": "./dist/cli.js",
  "bin": {
    "anima-storybook": "./dist/cli.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/assets",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AnimaApp/anima-storybook-cli"
  },
  "author": "Anima",
  "keywords": [
    "storybook-addons",
    "storybook",
    "addons",
    "figma",
    "cli",
    "storybook-to-figma",
    "anima",
    "figma",
    "design",
    "design system"
  ],
  "storybook": {
    "displayName": "Anima Storybook to Figma",
    "supportedFrameworks": [
      "react",
      "angular",
      "vue"
    ],
    "icon": "https://user-images.githubusercontent.com/1323193/157461790-52dc5857-2520-4ea8-84d0-bbeed32b4657.png"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.29.0",
    "@sentry/tracing": "^7.29.0",
    "@types/node-fetch": "^2.6.2",
    "archiver": "^5.3.1",
    "c12": "^0.2.13",
    "chalk": "^4.1.2",
    "flat": "^5.0.2",
    "form-data": "^4.0.0",
    "fs-extra": "10.0.0",
    "kebab-case": "^1.0.2",
    "lodash": "^4.17.21",
    "node-fetch": "2",
    "ora": "^5.4.1",
    "pkg-types": "^0.3.5",
    "yargs": "17.0.1",
    "zod": "^3.20.6",
    "@animaapp/framework-helpers": "1.0.2"
  },
  "devDependencies": {
    "@animaapp/token-core": "^1.0.16",
    "@jest/globals": "^29.4.2",
    "@types/archiver": "^5.3.1",
    "@types/flat": "^5.0.2",
    "@types/fs-extra": "9.0.12",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.5",
    "@types/yargs": "17.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "8.3.0",
    "jest": "^29.4.2",
    "prettier": "2.3.2",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "vitest": "^0.30.1"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "unit:test": "vitest run ./src/__tests__/",
    "test": "yarn run unit:test && node ./e2e-test/run.mjs",
    "lint:check": "eslint \"src/**/*.ts\""
  }
}